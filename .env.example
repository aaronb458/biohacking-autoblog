# OpenRouter API (for Claude Sonnet 4.5)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# PubMed API (optional but recommended to avoid rate limits)
PUBMED_API_KEY=

# DataForSEO API (for keyword research - optional, will use mock data if not set)
DATAFORSEO_USERNAME=
DATAFORSEO_PASSWORD=

# ZeroGPT API (for AI detection)
ZEROGPT_API_KEY=your_zerogpt_api_key_here

# WordPress Site
WORDPRESS_URL=https://your-wordpress-site.com
WORDPRESS_USERNAME=your_wp_username
WORDPRESS_APP_PASSWORD=your_wp_app_password

# Server Config
PORT=3000
NODE_ENV=development
